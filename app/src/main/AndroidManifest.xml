<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.co.chicmic.flyoverairways">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.CustomerDetailsActivity" />
        <activity android:name=".activities.NewUserInfoActivity" />
        <activity android:name=".activities.ChooseUserActivity" />
        <activity android:name=".activities.NewBookingActivity" />
        <activity android:name=".activities.ChooseFlightActivity" />
        <activity android:name=".activities.NewFlightActivity" />
        <activity android:name=".activities.CancelFlightActivity"/>
    </application>

</manifest>